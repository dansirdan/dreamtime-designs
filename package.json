{
  "name": "dreamtime-designs",
  "version": "0.0.1",
  "description": "Personal gallery portfolio for the artist, Melony Mont-Eton. (Dreamtime Designs)",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dansirdan/dreamtime-designs.git"
  },
  "keywords": [
    "portfolio",
    "artist",
    "gallery"
  ],
  "author": "Daniel Mont-Eton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dansirdan/dreamtime-designs/issues"
  },
  "homepage": "https://github.com/dansirdan/dreamtime-designs#readme",
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.26",
    "nodemailer": "^6.4.11",
    "nodemon": "^2.0.4"
  }
}
